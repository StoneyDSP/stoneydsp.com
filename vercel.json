{
	"version": 2,
	"devCommand": "yarn dev",
	"installCommand": "yarn install",
	"buildCommand": "yarn build",
	"outputDirectory": ".",
	"public": false,
	"framework": null,
	"functions": {
		"api/decode/index.js": {
			"memory": 1024,
			"maxDuration": 10
		},
		"api/encode/index.js": {
			"memory": 1024,
			"maxDuration": 10
		},
		"api/index.js": {
			"memory": 1024,
			"maxDuration": 10
		},
		"api/db/index.ts": {
			"memory": 1024,
			"maxDuration": 10
		}
	},
	"headers": [
		{
			"source": "/(.html)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "X-UA-Compatible",
					"value": "ie-edge"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-DNS-Prefetch-Control",
					"value": "on"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				},
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=63072000; includeSubDomains; preload"
				}
			]
		},
		{
			"source": "github-readme-stats.stoneydsp.com/(.*)",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "skillicons.dev/",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				}
			]
		},
		{
			"source": "/public/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				}
			]
		},
		{
			"source": "/:path*",
			"has": [
				{
					"type": "query",
					"key": "authorized"
				}
			],
		  "headers": [
				{
					"key": "x-authorized",
					"value": "true"
				}
			]
		},
		{
			"source": "/:path*",
			"missing": [
				{
					"type": "query",
					"key": "authorized"
				}
			],
		  "headers": [
				{
					"key": "x-authorized",
					"value": "false"
				}
			]
		}
	],
	"redirects": [
    {
      "source": "/.env",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/.git/",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/v1/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/v2/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/v3/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/v4/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/user/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/user/v1/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/user/v2/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/user/v3/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/user/v4/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/admin/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/admin/v1/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/admin/v2/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/admin/v3/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/admin/v4/.git/config",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/static/admin/javascript/hetong.js",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/Public/home/js/check.js",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/api/pin/",
			"destination": "https://github-readme-stats.vercel.app/api/pin/"
		},
		{
			"source": "/api/top-langs/",
			"destination": "https://github-readme-stats.vercel.app/api/top-langs/"
		},
		{
			"source": "/_vercel/insights/view",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		{
			"source": "/_vercel/insights/script.js",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		},
		,
		{
			"source": "/api/headers/index.js",
			"destination": "https://www.stoneydsp/api/headers?query=whoami"
		}
	]
}
